// 简化结构体数组测试
定义(结构体) Point {
    整型 x;
    整型 y;
};

定义(空类型) 主函数() {
    控制台输出("简化结构体数组测试");
    
    // 简单结构体数组定义
    定义(Point) points[2];
    
    // 赋值并输出
    points[0].x = 10;
    points[0].y = 20;
    
    points[1].x = 30;
    points[1].y = 40;
    
    控制台输出("Point[0]: (" + points[0].x + ", " + points[0].y + ")");
    控制台输出("Point[1]: (" + points[1].x + ", " + points[1].y + ")");
    控制台输出("结构体数组支持正常！");
}