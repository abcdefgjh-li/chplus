定义(空类型) 主函数() {
    控制台输出("=== 中文逻辑运算符测试 ===");
    
    定义(整型) a = 10;
    定义(整型) b = 20;
    
    // 测试"和"运算符
    如果 (a > 5 和 b < 30) {
        控制台输出("'和'条件满足: a > 5 且 b < 30");
    }
    
    // 测试"或"运算符
    如果 (a == 5 或 b == 20) {
        控制台输出("'或'条件满足: a == 5 或 b == 20");
    }
    
    // 测试复杂条件
    如果 ((a > 0 和 b > 0) 或 (a < 0 和 b < 0)) {
        控制台输出("复杂逻辑条件满足");
    }
    
    控制台输出("中文逻辑运算符测试完成");
}