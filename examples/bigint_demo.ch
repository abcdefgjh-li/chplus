定义(空类型) 主函数() {
    控制台输出("=== 大数组测试 ===");
    控制台换行();
    
    定义(整型) arr[10000];
    定义(整型) i;
    对于 (i = 0; i < 1000; i = i + 1) {
        arr[i] = i * 1000;
    }
    
    控制台输出("数组前1000个元素:");
    控制台换行();
    对于 (i = 0; i < 1000; i = i + 1) {
        控制台输出("arr[" + i + "] = " + arr[i]);
        控制台换行();
    }
}
