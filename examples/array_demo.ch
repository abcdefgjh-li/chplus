定义(空类型) 主函数() {
    // 数组定义和初始化
    定义(整型) numbers[5];
    定义(整型) i = 0;
    
    // 给数组赋值
    对于 (i = 0; i < 5; i = i + 1) {
        numbers[i] = (i + 1) * 10;
    }
    
    // 输出数组内容
    控制台输出("数组内容:");
    对于 (i = 0; i < 5; i = i + 1) {
        控制台输出("numbers[" + i + "] = " + numbers[i]);
    }
    
    // 数组计算
    定义(整型) sum = 0;
    对于 (i = 0; i < 5; i = i + 1) {
        sum = sum + numbers[i];
    }
    控制台输出("数组总和: " + sum);
}